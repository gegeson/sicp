>1;4205;0c(define (cont-frac n d k)
  (define (iter i)
    (if (= i k)
        (/ (n i) (d i))
        (/ (n i) (+ (d i) (iter (+ i 1)))))
    )
  (iter 0)
  )

(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  5))

(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  7))


(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  10))


(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  100))


(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  1000))


(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  1000000))

(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  10000))

(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  100000))

(print (cont-frac (lambda (i) 1.0)
                  (lambda (i) 1.0)
                  1000000))
